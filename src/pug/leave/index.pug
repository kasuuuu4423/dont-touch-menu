extends ../_layout

block config

block append head
	include ../_config
	link(rel="stylesheet", href="")
	link(rel="stylesheet", href=ROOT_PATH+"css/home.css")
	title #{titles['leave']}

block content
	header
		.container-fluid
			.row
				h1.col-12
					figure.logo: img(src=ROOT_PATH + "img/logo.jpg", alt="Don't Touch Menu")
	main.leave.container
			form#form.row(method="post", action="https://artful.jp/staging-menu/control/pdo/pdo_guest_delete.php")
					input#submit.col-12(type="button", value="退店")